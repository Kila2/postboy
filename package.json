{
  "name": "postman-like",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production babel-node --presets=env app",
    "start-dev": "cross-env PORT=8082 concurrently --kill-others \"nodemon --inspect --exec babel-node --presets=env ./backend/bin/www\" \"npm run build-dev\"",
    "build-dev": "webpack-cli --watch",
    "lint": "eslint .",
    "test": "npm run lint",
    "build": "webpack-cli",
    "livereload": "browser-sync start --config bs-config.js"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "csurf": "^1.9.0",
    "debug": "~2.6.9",
    "errorhandler": "^1.5.0",
    "express": "~4.15.5",
    "express-session": "^1.15.6",
    "mongodb": "^3.0.2",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "babel-core": "^6",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "~1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "bootstrap": "4",
    "clean-webpack-plugin": "^0.1.18",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.4.2",
    "cross-env": "^5.1.3",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.8"
  },
  "pre-commit": [
    "test"
  ]
}
